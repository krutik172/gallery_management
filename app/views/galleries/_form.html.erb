<div class="container">
<%= form_with model:@gallery do |form| %>
    
    <div class="form-group">
        <%= form.label :title %>
        <%= form.text_field :title, class: "form-control" %>
    </div>

    <div class="form-group">
         <%= form.label :image %>
        <%= form.file_field  :image , class: "form-control"%>
    </div>

    <div class="form-group"> 
        <%= form.submit class: "btn btn-success"%>
    </div>
<%end%>
</div>